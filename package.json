{
  "name": "pagopa-checkout-transactions-gateway-fe",
  "version": "0.0.1",
  "repository": "https://github.com/pagopa/pagopa-checkout-transactions-gateway-fe.git",
  "author": "PagoPA SpA",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "clean": "rimraf .cache && rimraf dist && shx rm -rf src/generated/pgs && shx rm -rf node_modules",
    "prebuild": "npm-run-all type-check lint",
    "build": "rimraf .cache && rimraf dist && parcel build src/index.html --public-url /payment-transactions-gateway",
    "start": "parcel src/index.html",
    "dev": "parcel --log-level verbose src/index.html",
    "generate": "npm-run-all -s generate:*",
    "generate:pgs-be": "shx rm -rf src/generated/pgs && shx mkdir -p src/generated/pgs && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/pagopa-infra/CHK-1407-update-pgs-apidef/src/core/api/payment_transactions_gateway_api/pgs-fe/v1/_openapi.json.tpl --no-strict --out-dir src/generated/pgs --request-types --response-decoders --client"
  },
  "devDependencies": {
    "@parcel/transformer-pug": "^2.6.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "api-spec-converter": "^2.12.0",
    "buffer": "^6.0.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sonarjs": "^0.5.0",
    "http-proxy-middleware": "^1.0.6",
    "italia-tslint-rules": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "os-browserify": "^0.3.0",
    "parcel": "^2.6.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "shx": "^0.3.4",
    "tslint": "^5.1.0",
    "typescript": "^4.1.2",
    "url": "^0.11.0",
    "util": "^0.12.5"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/base": "^5.0.0-alpha.85",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.86",
    "@mui/material": "^5.8.4",
    "@pagopa/mui-italia": "^0.6.26",
    "@pagopa/openapi-codegen-ts": "12.1.2",
    "dotenv": "^16.0.3",
    "i18next": "^21.8.9",
    "i18next-browser-languagedetector": "^6.1.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.17.2",
    "react-router-dom": "^6.6.1",
    "ts-node": "^10.9.1"
  }
}
